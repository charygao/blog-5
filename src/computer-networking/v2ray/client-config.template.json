{
  "log": {
    "logLevel": "warning"
  },
  "inbounds": [
    {
      "listen": "127.0.0.1",
      "port": 10080,
      "protocol": "http"
    },
    {
      "listen": "127.0.0.1",
      "port": 10081,
      "protocol": "socks",
      "settings": {
        "auth": "noauth",
        "udp": true
      }
    }
  ],
  "outbounds": [
    {
      "protocol": "vmess",
      "settings": {
        "vnext": [
          {
            "address": "127.0.0.1",
            "port": 443,
            "users": [
              {
                "id": "$UUID",
                "alterId": 16,
                "security": "auto"
              }
            ]
          }
        ]
      },
      "streamSettings": {
        "network": "tcp",
        "security": "tls",
        "tlsSettings": {
          "serverName": "$SERVER_NAME",
          "allowInsecure": false,
          "allowInsecureCiphers": false,
          "disableSystemRoot": true,
          "certificates": [
            $CERTIFICATE_JSON
          ]
        }
      }
    }
  ]
}
